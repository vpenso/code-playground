   "source": [
    "def decision(sum):\n",
    "    \"\"\"calculate a decision for sum\n",
    "        Input: sum\n",
    "        Output: 1, if sum >= 1,\n",
    "                0 otherwise\n",
    "    \"\"\"\n",
    "    if sum >= 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "\n",
    "result = decision(1)\n",
    "print(result)"
   ]
